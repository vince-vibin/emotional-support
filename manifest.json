{
    "name": "emotional support",
    "description": "dont work alone (get support from animal pictures)",
    "version": "0.8",
    "manifest_version": 3,
    
    "action": {
        "default_popup": "./popup/popup.html"
    },
    "permissions": [
        "storage"
    ],
    "icons": {
        "16": "./popup/data/logo16.png",
        "48": "./popup/data/logo48.png",
        "128": "./popup/data/logo128.png"
    },
    "background": {
        "service_worker": "./scripts/background.js"
    }
}